[
  { "label": "Result",
    "pattern": [
      {"LOWER": {"IN": ["auc","auroc","accuracy","precision","recall","f1","sensitivity","specificity","ppv","npv","rmse"]}}
    ]
  },
  { "label": "Result",
    "pattern": [
      {"LOWER": {"IN": ["odds","hazard"]}},
      {"LOWER": "ratio"}
    ]
  },
  { "label": "Result",
    "pattern": [
      {"LOWER": {"IN": ["confidence","ci"]}},
      {"LOWER": {"IN": ["interval","95%","90%","99%"]}}
    ]
  },
  { "label": "Result",
    "pattern": [
      {"LOWER": {"IN": ["compared","relative","versus","vs"]}},
      {"LOWER": {"IN": ["to","with"]}}
    ]
  },

  { "label": "Experiment",
    "pattern": [
      {"LOWER": {"IN": ["randomized","randomised","double-blind","single-blind","controlled","placebo-controlled","case-control","cohort","cross-sectional","prospective","retrospective"]}}
    ]
  },
  { "label": "Experiment",
    "pattern": [
      {"LOWER": {"IN": ["we","our","this"]}, "OP": "?"},
      {"LOWER": {"IN": ["conducted","performed","ran","administered","enrolled","recruited","assigned","randomized"]}}
    ]
  },

  { "label": "Technique",
    "pattern": [
      {"LOWER": {"IN": ["using","with","via","by","through"]}},
      {"IS_SPACE": true, "OP": "*"},
      {"IS_ALPHA": true, "OP": "+"}
    ]
  },

  { "label": "Dataset",
    "pattern": [
      {"LOWER": {"IN": ["clinicaltrials.gov","nct"]}},
      {"IS_ALPHA": true, "OP": "*"}
    ]
  },
  { "label": "Dataset",
    "pattern": [
      {"LOWER": {"IN": ["cohort","case-control","cross-sectional","prospective","retrospective"]}}
    ]
  },

  { "label": "Conclusion",
    "pattern": [
      {"LOWER": {"IN": ["in","overall"]}},
      {"LOWER": {"IN": ["conclusion","summary"]}}
    ]
  },
  { "label": "Conclusion",
    "pattern": [
      {"LOWER": {"IN": ["we","our","these"]}},
      {"LOWER": {"IN": ["conclude","confirm","support","suggest","indicate","underscore","highlight"]}}
    ]
  },

  { "label": "Input Fact",
    "pattern": [
      {"LOWER": {"IN": ["it","this"]}},
      {"LOWER": {"IN": ["is","was","has","have"]}},
      {"LOWER": {"IN": ["known","established","reported"]}},
      {"LOWER": "that"}
    ]
  }
]
